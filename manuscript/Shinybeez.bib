@article{franckAccurateCharacterizationDelay2015,
  title = {Accurate Characterization of Delay Discounting: {{A}} Multiple Model Approach Using Approximate Bayesian Model Selection and a Unified Discounting Measure},
  shorttitle = {Accurate Characterization of Delay Discounting},
  author = {Franck, Christopher T. and Koffarnus, Mikhail N. and House, Leanna L. and Bickel, Warren K.},
  year = {2015},
  journal = {Journal of the Experimental Analysis of Behavior},
  volume = {103},
  number = {1},
  pages = {218--233},
