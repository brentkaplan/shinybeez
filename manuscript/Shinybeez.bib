@article{koffarnusModifiedExponentialBehavioral2015a,
  title = {A Modified Exponential Behavioral Economic Demand Model to Better Describe Consumption Data},
  author = {Koffarnus, Mikhail N. and Franck, Christopher T. and Stein, Jeffrey S. and Bickel, Warren K.},
  year = {2015},
  journal = {Experimental and Clinical Psychopharmacology},
